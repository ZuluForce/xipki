<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<QAConf xmlns="http://xipki.org/qa/conf/v1">
  <X509Issuers>
    <X509Issuer name="RCA">
      <cert>
        <file>output/RCA1.der</file>
      </cert>
    </X509Issuer>
    <X509Issuer name="SubCA">
      <cert>
        <file>output/SubCA1.der</file>
      </cert>
      <ocspUrl>http://example.org/ocsp</ocspUrl>
    </X509Issuer>
    <X509Issuer name="SubCAwithCRL">
      <cert>
        <file>output/SubCAwithCRL1.der</file>
      </cert>
      <crlUrl>http://example.org/crl</crlUrl>
      <deltaCrlUrl>http://example.org/deltacrl</deltaCrlUrl>
    </X509Issuer>
  </X509Issuers>
  <X509CertProfiles>
    <X509CertProfile name="RCA">
      <file>demo/profile/CertProfile_RootCA.xml</file>
    </X509CertProfile>
    <X509CertProfile name="RCA-Cross">
      <file>demo/profile/CertProfile_RootCA_Cross.xml</file>
    </X509CertProfile>
    <X509CertProfile name="SubCA">
      <file>demo/profile/CertProfile_SubCA.xml</file>
    </X509CertProfile>
    <X509CertProfile name="SubCA.Complex">
      <file>demo/profile/CertProfile_SubCA_Complex.xml</file>
    </X509CertProfile>
    <X509CertProfile name="OCSP">
      <file>demo/profile/CertProfile_OCSP.xml</file>
    </X509CertProfile>
    <X509CertProfile name="multipleOUs">
      <file>demo/profile/CertProfile_multipleOUs.xml</file>
    </X509CertProfile>
    <X509CertProfile name="TLS">
      <file>demo/profile/CertProfile_TLS.xml</file>
    </X509CertProfile>
    <X509CertProfile name="TLS_C">
      <file>demo/profile/CertProfile_TLS_C.xml</file>
    </X509CertProfile>
    <X509CertProfile name="TLSwithIncSN">
      <file>demo/profile/CertProfile_TLSwithIncSN.xml</file>
    </X509CertProfile>
    <X509CertProfile name="gSMC_K">
      <file>demo/profile/CertProfile_gSMC_K.xml</file>
    </X509CertProfile>
  </X509CertProfiles>
  <environments>
    <environment name="env-a">
      <parameter name="add.prefix">true</parameter>
      <parameter name="add.suffix">true</parameter>
    </environment>
  </environments>
</QAConf>
