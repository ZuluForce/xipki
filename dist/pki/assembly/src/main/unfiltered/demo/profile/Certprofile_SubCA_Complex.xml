      </RSAParameters>
    </algorithm>
    <algorithm>
      <algorithm description="DSA">1.2.840.10040.4.1</algorithm>
      <DSAParameters>
        <pLength>
          <range min="1024" max="1024"/>
          <range min="2048" max="2048"/>
