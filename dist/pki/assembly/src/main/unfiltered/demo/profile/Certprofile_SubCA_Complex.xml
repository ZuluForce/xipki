      <type description="cn">2.5.4.3</type>
      <addPrefix>
        <condition operator="AND">
          <envParam>
            <name>add.prefix</name>
            <value>true</value>
          </envParam>
        </condition>
        <text>PREFIX </text>
      </addPrefix>
      <addSuffix>
        <condition operator="AND">
          <envParam>
            <name>add.suffix</name>
            <value>true</value>
          </envParam>
        </condition>
        <text> SUFFIX</text>
      </addSuffix>
